.nav_container {
    width: 600rpx;
    border: 1px solid #f5f5f5;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    position: absolute;
    bottom: 50rpx;
    left: 80rpx;
    height: 80rpx;
    background-color: #f7f7f7;
    border-radius: 10%;
}

.nav_container image {
    width: 80rpx;
    height: 80rpx;
}

.nav_container text {
    line-height: 80rpx;
    font-size: 24rpx;
}